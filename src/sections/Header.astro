---
const links = [
  {
    href: "#",
    label: "About us",
  },
  {
    href: "#",
    label: "Meetup",
  },
  {
    href: "#",
    label: "Youtube",
  },
  {
    href: "#",
    label: "Partners",
  },
  {
    href: "#",
    label: "Connect with us",
  },
];
---

<style>
  ul {
    display: flex;
    list-style: none;
    margin: 0;
    padding: 0;
    align-items: center;
  }

  a {
    color: white;
    text-decoration: none;
    padding: 1rem;
  }

  .wrapper {
    background: yellow;
    display: flex;
    justify-content: space-between;
    padding: 1rem;
    background: grey;
    color: white;
    align-items: center;
  }

  .logo {
    background: black;
    padding: 1rem;
  }
</style>

<div class="wrapper">
  <div class="logo">Logo</div>

  <nav>
    <ul>
      {
        links.map(({ label }) => (
          <li>
            <a href="#">{label}</a>
          </li>
        ))
      }
    </ul>
  </nav>
</div>
